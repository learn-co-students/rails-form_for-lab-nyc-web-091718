<h3>School Class Form</h3>

<%= form_for(@school_class) do |f| %>
  <label>Student Class title:</label><br>
  <%= f.text_field :title %><br>

  <label>Student Class room number</label><br>
  <%= f.text_field :room_number %><br>

  <%= f.submit %>
<% end %>


